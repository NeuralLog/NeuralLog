{
  "name": "@neurallog/integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for NeuralLog platform",
  "private": true,
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:ai": "jest --config jest.config.js --testPathPattern=ai",
    "test:api": "jest --config jest.config.js --testPathPattern=api",
    "test:auth": "jest --config jest.config.js --testPathPattern=auth",
    "test:ui": "jest --config jest.config.js --testPathPattern=ui",
    "test:ci": "jest --config jest.config.js --ci --coverage --watchAll=false"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.9",
    "@types/ws": "^8.5.8",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "node-fetch": "^2.6.7",
    "playwright": "^1.40.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "ws": "^8.14.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
